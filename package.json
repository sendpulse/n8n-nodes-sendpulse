{
  "name": "@sendpulse/n8n-nodes-sendpulse",
  "version": "0.0.2",
  "description": "n8n node for SendPulse API",
  "license": "MIT",
  "homepage": "https://sendpulse.com",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "sendpulse",
    "marketing"
  ],
  "author": {
    "name": "SendPulse",
    "email": "integrations@sendpulse.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sendpulse/n8n-nodes-sendpulse.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": [
      "dist/credentials/SendPulseApi.credentials.js",
      "dist/credentials/SendPulseOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/SendPulseA360/SendPulseA360.node.js",
      "dist/nodes/SendPulseChatbots/SendPulseChatbots.node.js",
      "dist/nodes/SendPulseCrm/SendPulseCrm.node.js",
      "dist/nodes/SendPulseEmail/SendPulseEmail.node.js",
      "dist/nodes/SendPulseFileManager/SendPulseFileManager.node.js",
      "dist/nodes/SendPulsePopUps/SendPulsePopUps.node.js",
      "dist/nodes/SendPulseSms/SendPulseSms.node.js",
      "dist/nodes/SendPulseSmtp/SendPulseSmtp.node.js",
      "dist/nodes/SendPulseVerifier/SendPulseVerifier.node.ts",
      "dist/nodes/SendPulseWebPush/SendPulseWebPush.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "@types/node": "^24.10.1",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
